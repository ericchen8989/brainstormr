{
  "name": "brainstormr",
  "version": "1.0.0",
  "description": "Brainstormr - AI-powered writing helper that analyzes and creates professional content based on quick one-line summaries.",
  "author": "brainstormr",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "audit": "better-npm-audit audit",
    "tinymcepostinstall": "node ./postinstall.js",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
    "check-style:prettier": "prettier --check ./components ./pages ./styles ./utils",
    "make-style:prettier": "prettier --write ./components ./pages ./styles ./utils",
    "prepare": "husky install",
    "start": "next start",
    "test-unit": "jest",
    "watch-sass": "sass --watch styles/style.scss styles/style.css",
    "markdown-notes-tree": "npx markdown-notes-tree --ignore **/LICENSE.md --ignore **/CHANGELOG.md --ignore lib-cov --ignore coverage --ignore build/Release --ignore node_modules/ --ignore out --ignore public/tinymce --ignore src --ignore build --ignore bower_components --ignore npm-debug.log* --ignore yarn-debug.log* --ignore yarn-error.log* --ignore firebase-debug.log* --ignore logs pids"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "echo \"[Husky] pre-commit\"",
      "pre-merge-commit": "echo \"[Husky] pre-merge-commit\"",
      "prepare-commit-msg": "echo \"[Husky] prepare-commit-msg\"",
      "commit-msg": "echo \"[Husky] commit-msg\"",
      "post-commit": "echo \"[Husky] post-commit\"",
      "pre-rebase": "echo \"[Husky] pre-rebase\"",
      "post-checkout": "echo \"[Husky] post-checkout\"",
      "post-merge": "echo \"[Husky] post-merge\"",
      "pre-push": "echo \"[Husky] pre-push\"",
      "pre-receive": "echo \"[Husky] pre-receive\"",
      "update": "echo \"[Husky] update\"",
      "post-receive": "echo \"[Husky] post-receive\"",
      "post-update": "echo \"[Husky] post-update\"",
      "push-to-checkout": "echo \"[Husky] push-to-checkout\"",
      "pre-auto-gc": "echo \"[Husky] pre-auto-gc\"",
      "post-rewrite": "echo \"[Husky] post-rewrite\"",
      "sendemail-validate": "echo \"[Husky] sendemail-validate\"",
      "fsmonitor-watchman": "echo \"[Husky] fsmonitor-watchman\"",
      "p4-pre-submit": "echo \"[Husky] p4-pre-submit\"",
      "post-index-change": "echo \"[Husky] post-index-change\"",
      "p4-changelist": "echo \"[Husky] p4-changelist\"",
      "p4-post-changelist": "echo \"[Husky] p4-post-changelist\"",
      "p4-prepare-changelist": "echo \"[Husky] p4-prepare-changelist\"",
      "reference-transaction": "echo \"[Husky] reference-transaction\""
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.5.0",
    "@tinymce/tinymce-react": "^4.2.0",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "fs-extra": "^10.1.0",
    "fslightbox-react": "^1.6.2-2",
    "local-storage": "^2.0.0",
    "next": "^12.2.5",
    "next-auth": "^4.15.0",
    "nodemailer": "^6.7.7",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-select": "^5.4.0",
    "sass": "^1.54.4",
    "sweetalert2": "^11.4.19",
    "sweetalert2-react-content": "^5.0.0",
    "swiper": "^8.3.2",
    "tinymce": "^6.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.7.18",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.0.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "markdown-notes-tree": "^1.12.0",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "prisma": "^4.5.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.8"
  }
}
